{
  "name": "styled-twbs",
  "version": "0.0.1",
  "description": "styled-twbs",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-s build:clean build:src build:lib",
    "build:clean": "rm -rf src/*.js lib",
    "build:src": "node ./bin/build",
    "build:lib": "better-npm-run build:lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "build:lib": {
      "command": "babel src -d lib",
      "env": {
        "BABEL_ENV": "es6"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amurdock/styled-twbs.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amurdock/styled-twbs/issues"
  },
  "homepage": "https://github.com/amurdock/styled-twbs#readme",
  "peerDependencies": {
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "bootstrap": "4.0.0-alpha.6",
    "camel-case": "^3.0.0",
    "gonzales-pe": "^4.0.3",
    "javascript-stringify": "^1.6.0",
    "npm-run-all": "^4.0.2"
  },
  "dependencies": {}
